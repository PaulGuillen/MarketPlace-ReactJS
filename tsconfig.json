{
  "compilerOptions": {
    "jsx": "react-jsx",  
    "esModuleInterop": true, 
    "allowSyntheticDefaultImports": true,
    "strict": false, 
    "noImplicitAny": false, 
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": false,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
